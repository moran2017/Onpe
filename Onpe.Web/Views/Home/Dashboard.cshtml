    <!---HighChart--->


        <!--Librerias de jquery---->
   
    <script src=@Url.Content("~/Content/js/Highcharts-3.0.10/js/highcharts.js") ></script>
    <script src=@Url.Content("~/Content/js/Highcharts-3.0.10/js/highcharts.src.js")></script>
    <script src=@Url.Content("~/Content/js/Highcharts-3.0.10/js/modules/exporting.js")></script>
  
    
    <script type="text/javascript">

      Highcharts.chart('Grafica', {

          title: {
              text: 'Logarithmic axis demo'
          },

          xAxis: {
              tickInterval: 1
          },

          yAxis: {
              type: 'logarithmic',
              minorTickInterval: 0.1
          },

          tooltip: {
              headerFormat: '<b>{series.name}</b><br />',
              pointFormat: 'x = {point.x}, y = {point.y}'
          },

          series: [{
              data: [1, 2, 4, 8, 16, 32, 64, 128, 256, 512],
              pointStart: 1
          }]
      });
    </script>





    
        